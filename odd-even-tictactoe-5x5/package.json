{
  "name": "odd-even-tictactoe-5x5",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "install-all": "npm --prefix server install && npm --prefix client install",
    "dev": "concurrently -n SERVER,CLIENT -c green,cyan \"npm --prefix server run dev\" \"npm --prefix client run dev\"",
    "build": "npm --prefix client run build",
    "build-single-serve": "node build-single-serve.js",
    "start": "npm --prefix server run start"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.2",
    "concurrently": "^9.0.0",
    "esbuild": "^0.25.11",
    "rollup": "^4.52.4",
    "rollup-plugin-string": "^3.0.0"
  },
  "dependencies": {
    "express": "^4.21.2",
    "ws": "^8.18.3"
  }
}
